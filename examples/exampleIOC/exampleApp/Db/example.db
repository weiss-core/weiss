record(calc, "$(P)$(R)Secs-Mon") {
  field(DESC, "IOC uptime")
  field(INPA, "$(P)$(R)Secs-Mon")
  field(CALC, "A+0.1")
  field(SCAN, ".1 second")
  field(PREC, "3")
  field(EGU, "s")
}

record(ao, "$(P)$(R)Freq-SP") {
  field(DESC, "Sin Frequency")
  field(VAL, "0.5")
  field(DRVH, "1") # Limited freq due to 10Hz update on seconds
  field(HOPR, "1")
  field(DRVL, "0")
  field(LOPR, "0")
  field(EGU, "Hz")
}

record(ao, "$(P)$(R)Ampl-SP") {
  field(DESC, "Sin Amplitude")
  field(VAL, "5000")
  field(DRVH, "10000")
  field(HOPR, "10000")
  field(DRVL, "0")
  field(LOPR, "0")
}

record(ai, "$(P)$(R)Ampl-RB") {
  field(DESC, "Sin Amplitude Readback")
  field(INP, "$(P)$(R)Ampl-SP CP")
  field(HIHI, "9000")
  field(HHSV, "MAJOR")
  field(HIGH, "8000")
  field(HSV, "MINOR")
  field(LOLO, "1000")
  field(LLSV, "MAJOR")
  field(LOW, "2000")
  field(LSV, "MINOR")
}

record(compress, "$(P)$(R)AmplArr-Mon") {
  field(DESC, "Waves Amplitude Arr")
  field(ALG, "Circular Buffer")
  field(INP, "$(P)$(R)Ampl-RB")
  field(SCAN, ".1 second")
  field(NSAM, "100")
}

record(calc, "$(P)$(R)Sin-Mon") {
  field(DESC, "Sine wave")
  field(INPA, "$(P)$(R)Secs-Mon CP")
  field(INPB, "$(P)$(R)Ampl-SP")
  field(INPC, "$(P)$(R)Freq-SP")
  field(CALC, "B*SIN(2*PI*C*A)")
  field(PREC, "3")
  field(VAL, "0")
  field(EGU, "V")
  field(MDEL, "-1")
}

record(compress, "$(P)$(R)SinArr-Mon") {
  field(DESC, "Sine array")
  field(ALG, "Circular Buffer")
  field(INP, "$(P)$(R)Sin-Mon CP")
  field(NSAM, "100")
}

record(calc, "$(P)$(R)Cos-Mon") {
  field(DESC, "Cosine wave")
  field(INPA, "$(P)$(R)Secs-Mon CP")
  field(INPB, "$(P)$(R)Ampl-SP")
  field(INPC, "$(P)$(R)Freq-SP")
  field(CALC, "B*COS(2*PI*C*A)")
  field(PREC, "3")
  field(VAL, "0")
  field(EGU, "V")
  field(MDEL, "-1")
}

record(compress, "$(P)$(R)CosArr-Mon") {
  field(DESC, "Cosine array")
  field(ALG, "Circular Buffer")
  field(INP, "$(P)$(R)Cos-Mon CP")
  field(NSAM, "100")
}

